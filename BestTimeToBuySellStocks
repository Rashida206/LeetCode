class Solution {
public:
    int maxProfit(vector<int>& prices) {
          int minPrice = prices[0], max = 0, x=prices.size();
        for (int i = 1; i < prices.size(); i++) {
            if (prices[i] < minPrice) 
                minPrice = prices[i];
            else if (prices[i] - minPrice > max) 
                max = prices[i] - minPrice;
        } 
        
    return max;

    } 
};